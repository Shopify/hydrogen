{
  "compilerOptions": {
    "checkJs": false,
    "target": "ES2022",
    "module": "ES2022",
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "app/*"
      ]
    }
  },
  "include": [
    "env.d.ts",
    "app/**/*.js",
    "app/**/*.jsx",
    "app/**/*.d.ts",
    "*.js",
    "*.jsx",
    "*.d.ts",
    ".graphqlrc.js",
    ".react-router/types/**/*"
  ]
}